<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc><%= absolute('/') %></loc>
		<changefreq>hourly</changefreq>
	</url>
	<url>
		<loc><%= absolute('/contact') %></loc>
	</url>
	<url>
		<loc><%= absolute('/partners') %></loc>
	</url>

	<% for (let bot of bots) { %>
	<url>
		<loc><%= absolute(`/bots/${bot.discord_id}`) %></loc>
		<lastmod><%= bot.updated_at.toISOString() %></lastmod>
	</url>
	<% } %>

	<% for (let user of users) { %>
	<url>
		<loc><%= absolute(`/users/${user.discord_id}`) %></loc>
		<lastmod><%= user.updated_at.toISOString() %></lastmod>
	</url>
	<% } %>

	<% for (let tag of tags) { %>
	<url>
		<loc><%= absolute(`/tags/${tag.name}`) %></loc>
		<lastmod><%= tag.updated_at.toISOString() %></lastmod>
	</url>
	<% } %>
</urlset> 
